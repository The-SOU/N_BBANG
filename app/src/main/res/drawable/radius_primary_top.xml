<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <gradient
                android:type="linear"
                android:angle="45"
                android:startColor="@color/colorPrivateStart"
                android:endColor="@color/colorPrivateEnd"
                />
            <corners android:topLeftRadius="20dp" android:topRightRadius="20dp"/>
        </shape>
    </item>

</layer-list>